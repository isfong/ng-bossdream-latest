{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "types": [
      "jasmine",
      "node"
    ],
    "baseUrl": "./",
    "paths": {
      "@app": [
        "app"
      ],
      "@app/*": [
        "app/*"
      ],
      "@com": [
        "app/components"
      ],
      "@com/*": [
        "app/components/*"
      ],
      "@mod": [
        "app/models"
      ],
      "@mod/*": [
        "app/models/*"
      ],
      "@ser": [
        "app/services"
      ],
      "@ser/*": [
        "app/services/*"
      ],
      "@sha": [
        "app/shared"
      ],
      "@sha/*": [
        "app/shared/*"
      ],
      "@tes": [
        "testing/"
      ],
      "@tes/*": [
        "testing/*"
      ],
      "@env": [
        "environments/"
      ],
      "@env/*": [
        "environments/*"
      ],
      "@e2e": [
        "../e2e"
      ],
      "@e2e/*": [
        "../e2e/*"
      ]
    }
  },
  "files": [
    "test.ts",
    "polyfills.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
